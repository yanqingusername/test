.P6{
    height: 55px;
    background-color: #fff;
    display: flex;
    border-radius: 40rpx;
}
.qitai {
    letter-spacing: 1rpx;
    color: #327FF6;
    font-size: 55rpx;
    display: flex;
    /*设置显示样式**/
    align-items: center;
    /**子view垂直居中*/
    vertical-align: center;
    /**垂直居中*/
    justify-content: center;
    /**内容居中*/
    flex-direction: row;
    width: 100%;
}

.eng {
    width: 100%;
    display: flex;
    /*设置显示样式**/
    align-items: center;
    /**子view垂直居中*/
    vertical-align: center;
    /**垂直居中*/
    justify-content: center;
    /**内容居中*/
    flex-direction: row;
    color: #327FF6;
    font-size: 32rpx;
}